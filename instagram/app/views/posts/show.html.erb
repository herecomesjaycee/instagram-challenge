<p><%= @post.caption%></p>
<%= image_tag 'https:' + @post.image.url[0..4] + 'eu-west-2.' + @post.image.url(:medium)[5..@post.image.url.length-1] %>
<% @comments.each do |comment| %>
  <div class="comment">
    <h4> <%= comment.user_id%><%= comment.content %> </h4>
  </div>
  <% end %>
  <%= link_to "Back to homepage", root_path %>
